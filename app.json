{
  "expo": {
    "name": "Churrapp",
    "slug": "Churrapp",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "churrapp",
    "version": "0.9.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.churrappbrasil.churrapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.churrappbrasil.churrapp",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "churrapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    }
  }
}